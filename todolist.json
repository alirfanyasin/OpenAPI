      "delete": {
        "summary": "Delete Todolist",
        "description": "Delete todolist by id to database",
        "parameters": [
          {
            "name": "todolistId",
            "in": "path",
            "required": true,
            "description": "todolist delete by id",
            "schema": {
              "type": "string",
              "minLength": 1,
              "maxLength": 100
