  "paths": {
    "/todolist": {
      "get": {
        "summary": "Get All Todolist",
        "description": "Get all data todolist in database",
        "parameters": [
          {
            "name": "include_done",
            "in": "query",
            "required": false,
            "description": "Is include done todolist",
            "schema": {
              "type": "boolean",
              "nullable": true,
              "default": false
            }
          },
          {
            "name": "name",
            "in": "query",
            "required": false,
            "description": "Search todolist by name",
            "schema": {
              "type": "string",
              "nullable": true,
              "minLength": 1,
              "maxLength": 100
            },
            "examples": {
              "java": {
                "description": "Example name java",
                "value": "java"
              },
              "php": {
                "description": "Example name php",
                "value": "php"
              },
              "JavaScript": {
                "description": "Example name JavaScript",
                "value": "JavaScript"
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success get all todolist",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string"
                      },
                      "name": {
                        "type": "string"
                      },
                      "priority": {
                        "type": "number",
                        "format": "int32"
                      },
                      "tags": {
                        "type": "array",
                        "items": {
                          "type": "string"
                        }
                      }
                    }
                  }
                },
                "examples": {
                  "success": {
                    "description": "Example success get all todolist",
                    "value": [
                      {
                        "id": "1",
                        "name": "Java",
                        "priority": 2,
                        "tags": ["Java", "Progarmming"]
                      },
                      {
                        "id": "2",
                        "name": "PHP",
                        "priority": 4,
                        "tags": ["PHP", "Hypertext Preprocessor", "Progarmming"]
                      }
                    ]
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "summary": "Create New Todolist",
        "description": "Create new todolist to database",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 100
                  },
                  "priority": {
                    "type": "integer",
                    "format": "int32",
                    "default": 1
                  },
                  "tags": {
                    "type": "array",
                    "minItems": 1,
                    "items": {
                      "type": "string",
                      "minLength": 1
                    }
                  }
                }
              },
              "examples": {
                "java": {
                  "description": "Example create todolist Java",
                  "value": {
                    "name": "Java",
                    "priority": 2,
                    "tags": ["Java", "Progarmming"]
                  }
                },
                "PHP": {
                  "description": "Example create todolist PHP",
                  "value": {
                    "name": "PHP",
                    "priority": 4,
                    "tags": ["PHP", "Hypertext Preprocessor", "Progarmming"]
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success create new todolist",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string"
                    },
                    "name": {
                      "type": "string"
                    },
                    "priority": {
                      "type": "number",
                      "format": "int32"
                    },
                    "tags": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    }
                  }
                },
                "examples": {
                  "java": {
                    "description": "Success create data Java",
                    "value": {
                      "id": "1",
                      "name": "Java",
                      "priority": 2,
                      "tags": ["Java", "Progarmming"]
                    }
                  },
                  "php": {
                    "description": "Success create data php",
                    "value": {
                      "id": "2",
                      "name": "PHP",
                      "priority": 4,
                      "tags": ["PHP", "Hypertext Preprocessor", "Progarmming"]
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
